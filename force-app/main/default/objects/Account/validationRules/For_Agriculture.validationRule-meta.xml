<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>For_Agriculture</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Industry, &quot;Agriculture&quot; ), ISBLANK( Fax ))</errorConditionFormula>
    <errorDisplayField>Fax</errorDisplayField>
    <errorMessage>Please fillup fax.</errorMessage>
</ValidationRule>
